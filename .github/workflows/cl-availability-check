name: cl-check.yml
on:
  workflow_dispatch:

jobs:
  build:
    name: Check
    runs-on: windows-latest
    
    steps:
      - uses: actions/checkout@v4

      - uses: graalvm/setup-graalvm@v1
        with:
          java-version: '21'
          distribution: 'graalvm'
          github-token: ${{ secrets.GITHUB_TOKEN }}
          native-image-job-reports: 'true'
        
      - name: cl-check
        run: cl
        shell: cmd

      
